   "source": [
    "import jax\n",
    "import jax.numpy as jnp\n",
    "import jax.random as jr"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "27015920",
   "metadata": {},
   "outputs": [],
   "source": [
    "key = jr.PRNGKey(42)\n",
    "\n",
    "eps = jax.random.normal(key, shape=[10000])\n",
    "\n",
    "# t inherits the sharding from eps\n",
    "t = jax.nn.sigmoid(eps)\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "plt.hist(t, bins=50)\n",
    "plt.show()"
   ]
  },
